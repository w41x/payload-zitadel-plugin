{
  "name": "payload-zitadel-plugin",
  "version": "0.2.42",
  "description": "plugin for Payload CMS, which enables authentication via Zitadel IdP",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "WALX Tech e.K.",
    "email": "x@walx.tech",
    "url": "https://walx.tech"
  },
  "contributors": [
    {
      "name": "ле41%",
      "email": "_@w41x.io",
      "url": "https://w41x.io"
    }
  ],
  "homepage": "https://github.com/w41x/payload-zitadel-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w41x/payload-zitadel-plugin.git"
  },
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "zitadel"
  ],
  "bugs": {
    "url": "https://github.com/w41x/payload-zitadel-plugin/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:swc && pnpm build:types",
    "build:swc": "swc src -d dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "reset": "rimraf dist node_modules pnpm-lock.yaml tsconfig.tsbuildinfo dev/.next dev/node_modules dev/pnpm-lock.yaml",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@payloadcms/next": "3.0.0-beta.128",
    "@payloadcms/translations": "3.0.0-beta.128",
    "@payloadcms/ui": "3.0.0-beta.128",
    "jose": "^5.9.6",
    "next": "^15.0.4-canary.6",
    "payload": "3.0.0-beta.128",
    "react": "19.0.0-rc-2ec26bc4-20241111",
    "react-dom": "19.0.0-rc-2ec26bc4-20241111"
  },
  "devDependencies": {
    "@swc/cli": "^0.5.0",
    "@swc/core": "^1.9.2",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "typescript": "5.8.0-dev.20241112"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.0",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
    }
  },
  "engines": {
    "node": "^22.11.0"
  },
  "packageManager": "pnpm@9.12.3",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./components": {
        "import": "./dist/components/index.js",
        "require": "./dist/components/index.js",
        "types": "./dist/components/index.d.ts"
      },
      "./types": {
        "import": "./dist/types.js",
        "require": "./dist/types.js",
        "types": "./dist/types.d.ts"
      }
    },
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org",
    "types": "./dist/index.d.ts"
  }
}
