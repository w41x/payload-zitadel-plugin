{
  "name": "payload-zitadel-plugin",
  "version": "0.2.16",
  "description": "plugin for Payload CMS, which enables authentication via Zitadel IdP",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "WALX Tech e.K.",
    "email": "x@walx.tech",
    "url": "https://walx.tech"
  },
  "contributors": [
    {
      "name": "ле41%",
      "email": "_@w41x.io",
      "url": "https://w41x.io"
    }
  ],
  "homepage": "https://github.com/w41x/payload-zitadel-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w41x/payload-zitadel-plugin.git"
  },
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "zitadel"
  ],
  "bugs": {
    "url": "https://github.com/w41x/payload-zitadel-plugin/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:swc && pnpm build:types",
    "build:swc": "swc src -d dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "reset": "rimraf dist node_modules pnpm-lock.yaml tsconfig.tsbuildinfo dev/.next dev/node_modules dev/pnpm-lock.yaml",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@payloadcms/next": "3.0.0-beta.84",
    "@payloadcms/translations": "3.0.0-beta.84",
    "@payloadcms/ui": "3.0.0-beta.84",
    "jsonwebtoken": "^9.0.2",
    "next": "15.0.0-canary.119",
    "payload": "3.0.0-beta.84",
    "react": "rc",
    "react-dom": "rc"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.4.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@rc",
      "@types/react-dom": "npm:types-react-dom@rc"
    }
  },
  "engines": {
    "node": "^22.6.0"
  },
  "packageManager": "pnpm@9.7.1",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./client": {
        "import": "./dist/client.js",
        "require": "./dist/client.js",
        "types": "./dist/client.d.ts"
      },
      "./hooks": {
        "import": "./dist/hooks/index.js",
        "require": "./dist/hooks/index.js",
        "types": "./dist/hooks/index.d.ts"
      },
      "./types": {
        "import": "./dist/types.js",
        "require": "./dist/types.js",
        "types": "./dist/types.d.ts"
      }
    },
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org",
    "types": "./dist/index.d.ts"
  }
}
